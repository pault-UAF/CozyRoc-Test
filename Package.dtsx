		public void Main()
		{
            string dirPath = (string)Dts.Variables["$Project::SourcePath"].Value;
            string[] aFiles = Directory.GetFiles(dirPath);
            List<string> lFiles = new List<string>();
            foreach (string fileName in aFiles)
